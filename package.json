{
  "name": "DevEnv-Node",
  "version": "1.0.0",
  "description": "Base files for new Node project!",
  "scripts": {
    "preinstall": "npm-run-all parcel:build",
    "start": "npm-run-all nodemon-dev",
    "code": "npm-run-all --parallel parcel nodemon-dev",
    "code:build": "npm-run-all --sequential env-prod del-dist build nodemon-prod",
    "Comment": "//////////////// Build Scripts",
    "build": "npm-run-all --sequential parcel:build security-check",
    "nodemon-dev": "cross-env NODE_ENV=development nodemon ./dist/app.js",
    "nodemon-prod": "cross-env NODE_ENV=production nodemon ./dist/app.js",
    "debug": "nodemon --inspect-brk ./dist/app.js",
    "parcel": "parcel watch app.js --public-url ./ --target node",
    "parcel:build": "parcel build app.js --public-url ./ --target node",
    "security-check": "nsp check"
  },
  "author": "Anthony White",
  "license": "",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.4.1",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "hbs": "^4.0.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "babel-core": "6.17.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "6.24.1",
    "build": "npm-run-all --sequential parcel:build security-check",
    "cross-env": "^5.2.0",
    "eslint": "^3.19.0",
    "eslint-config-google": "0.9.1",
    "eslint-config-problems": "0.1.1",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "^2.12.0",
    "nodemon": "^1.18.6",
    "npm-run-all": "^3.1.2",
    "nsp": "2.6.2",
    "parcel-bundler": "^1.10.3"
  }
}
