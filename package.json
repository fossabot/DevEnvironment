    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "hbs": "^4.0.1",
    "mongodb": "^3.1.10",
    "mongoose": "^5.3.14",
    "nodemon": "^1.18.6",
    "npm-run-all": "^3.1.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "swift-log": "^1.0.1"
  },
  "devDependencies": {
    "babel-core": "6.17.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-rewire": "^1.2.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "6.24.1",
    "cross-env": "^5.2.0",
    "del-cli": "^1.1.0",
    "eslint": "^3.19.0",
    "eslint-config-google": "0.9.1",
    "eslint-config-problems": "0.1.1",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "^2.12.0",
